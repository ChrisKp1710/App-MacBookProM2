{
  "name": "myelectronapp",
  "version": "1.0.0",
  "description": "My Electron App built with Vite and Electron Builder",
  "author": "Christian Koscielniak Pinto",
  "license": "MIT",
  "main": "dist/main.js",
  "private": true,
  "devDependencies": {
    "@electron/typescript-definitions": "^8.14.0",
    "@rollup/plugin-commonjs": "^25.0.0",
    "@types/better-sqlite3": "^7.6.12",
    "@types/node": "^22.10.5",
    "concurrently": "^9.1.2",
    "electron": "^33.2.1",
    "electron-builder": "^25.1.8",
    "electron-rebuild": "^3.2.9",
    "electronmon": "^2.0.3",
    "vite": "^6.0.7",
    "vite-plugin-electron": "^0.29.0",
    "vite-plugin-electron-renderer": "^0.14.6"
  },
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "start": "electron dist/main.js",
    "pack": "electron-builder --dir",
    "dist": "yarn full-build && electron-builder",
    "postbuild": "node scripts/postbuild.js",
    "full-build": "yarn build && yarn postbuild",
    "rebuild": "electron-rebuild -f -w better-sqlite3"
  },
  "dependencies": {
    "@codemirror/basic-setup": "^0.20.0",
    "@codemirror/lang-html": "^6.4.9",
    "@codemirror/lang-json": "^6.0.1",
    "@codemirror/lang-markdown": "^6.3.1",
    "@codemirror/state": "^6.5.0",
    "@codemirror/view": "^6.36.1",
    "better-sqlite3": "^11.7.2"
  },
  "resolutions": {
    "@codemirror/state": "^6.5.0"
  }
}
